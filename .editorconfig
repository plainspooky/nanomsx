# Editor configuration, see https://editorconfig.org
root = true

[Makefile]
indent_style = tab

[*.{inc,pas}]
end_of_line = crlf
charset = latin1
indent_style = space
indent_size = 2
insert_final_newline = false
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
